{
    "manifest_version": 2,
    "name": "Moodle fucker",
    "author": "Taras Labiak",
    "description": "",
    "version": "0.1",
    "icons": {
        "16": "/img/logo16.png",
        "128": "/img/logo128.png"
    },
    "content_scripts": [{
        "matches": ["http://moodle.tdmu.edu.ua/*"],
        "js": ["embed.js"],
        "css": ["embed.css"],
        "run_at": "document_start"
    }],
    "browser_action": {
        "default_title": "Fuck",
        "default_icon": "img/logo128.png",
        "default_popup": "popup.html"
    },
    "permissions": [
        "tabs",
        "activeTab",
        "contextMenus",
        "webNavigation",
        "webRequest",
        "http://*/",
        "https://*/"
    ]
}